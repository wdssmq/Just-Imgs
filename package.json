{
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --mode dev --host --port 3333",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "init:env": "cp .env.sample .env.dev.local && cp .env.sample .env.local",
    "postinstall": "npm run init:env && npx simple-git-hooks"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.19",
    "@vueuse/core": "^10.1.2",
    "vue": "^3.3.4",
    "vue-router": "^4.2.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.5",
    "@iconify-json/carbon": "^1.1.17",
    "@types/node": "^20.2.5",
    "@unocss/eslint-config": "^0.53.0",
    "@unocss/reset": "^0.53.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/volar": "^0.10.2",
    "@vue/test-utils": "^2.3.2",
    "daisyui": "^3.1.6",
    "eslint": "^8.42.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.2.2",
    "pnpm": "^8.6.8",
    "sass": "^1.63.6",
    "simple-git-hooks": "^2.8.1",
    "taze": "^0.10.2",
    "typescript": "^5.1.3",
    "unocss": "^0.53.0",
    "unocss-preset-daisy": "^5.0.0",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-vue-components": "^0.25.0",
    "unplugin-vue-macros": "^2.2.1",
    "vite": "^4.3.9",
    "vite-plugin-pages": "^0.31.0",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.6.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}

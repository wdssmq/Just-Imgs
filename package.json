{
  "private": true,
  "packageManager": "pnpm@8.6.0",
  "scripts": {
    "build": "vite build",
    "dev": "vite --mode dev --host --port 3333",
    "lint": "eslint .",
    "typecheck": "vue-tsc --noEmit",
    "preview": "vite preview",
    "test": "vitest",
    "up": "taze major -I",
    "init:env": "cp .env.sample .env.dev.local && cp .env.sample .env.local",
    "postinstall": "npm run init:env && npx simple-git-hooks"
  },
  "dependencies": {
    "@fancyapps/ui": "^5.0.36",
    "@vueuse/core": "^10.11.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@iconify-json/carbon": "^1.2.14",
    "@types/node": "^20.19.25",
    "@unocss/eslint-config": "^0.53.6",
    "@unocss/reset": "^0.53.6",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue-macros/volar": "^0.10.2",
    "@vue/test-utils": "^2.4.6",
    "daisyui": "^3.9.4",
    "eslint": "^8.57.1",
    "jsdom": "^22.1.0",
    "lint-staged": "^13.3.0",
    "pnpm": "^8.15.9",
    "sass": "^1.94.2",
    "simple-git-hooks": "^2.13.1",
    "taze": "^0.10.3",
    "typescript": "^5.9.3",
    "unocss": "^0.53.6",
    "unocss-preset-daisy": "^5.0.0",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "unplugin-vue-macros": "^2.14.5",
    "vite": "^4.5.14",
    "vite-plugin-pages": "^0.31.0",
    "vitest": "^0.31.4",
    "vue-tsc": "^1.8.27"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "eslintConfig": {
    "extends": [
      "@antfu",
      "@unocss"
    ]
  }
}
